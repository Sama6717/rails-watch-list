<h2><%= @list.name %></h2>
 <%= link_to "Back" , lists_path, class:"btn btn-default"%>
 <%= link_to "Add Bookmark" ,new_list_bookmark_path(@list), class:"btn btn-default"%>

<br>

 <tbody>
    <% @list.bookmarks.each do |bookmark| %>
      <tr>
        <td><%= bookmark.comment %></td>
        <td><%= bookmark.movie.title %></td>
    </tr>
<% end %>
</tbody>
